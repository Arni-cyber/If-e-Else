#include <stdio.h>

int main() {
    int número;
    
    printf("Digite a Nota: ");
    scanf("%d",&número);
    
    if (número >= 70){
    printf("A nota %d Passou");
    }
    else if(número >= 50){
    printf("A nota %d Reprovado");
    
    if (número <= 69)
      printf("A nota %d Recuperação");
    }
    
    return 0;
}
